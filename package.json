{
  "name": "oauth2-client-shell-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "cd": "npm run test && npm run build",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:debug": "vue-cli-service test:unit --inspect-brk --watch",
    "lint": "vue-cli-service lint",
    "start": "http-server dist/ -o -S -C node_modules/openssl-self-signed-certificate/cert.pem -K node_modules/openssl-self-signed-certificate/key.pem",
    "test": "npm run test:unit",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "bootstrap-vue": "^2.21.2",
    "bootswatch": "^4.6.0",
    "core-js": "^3.10.0",
    "csprng": "^0.1.2",
    "vue": "^2.6.12",
    "vue-clipboard2": "^0.3.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-unit-mocha": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/test-utils": "1.1.3",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.6.0",
    "chai": "^4.3.4",
    "eslint": "^7.23.0",
    "eslint-plugin-vue": "^7.8.0",
    "http-server": "^0.12.3",
    "mutationobserver-shim": "^0.3.7",
    "node-sass": "^5.0.0",
    "openssl-self-signed-certificate": "^1.1.6",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "sass-loader": "^11.0.1",
    "vue-cli-plugin-bootstrap-vue": "^0.7.0",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "semi": [
        "warn",
        "always"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
